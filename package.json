    "@material/mwc-top-app-bar-fixed": "^0.8.0",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "lodash": "^4.17.19",
    "pwa-helpers": "^0.9.0",
    "random-words": "^1.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "tokenizer.js": "^1.3.2"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.3.4",
    "@open-wc/testing-karma": "^3.1.33",
