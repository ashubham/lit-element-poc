    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "deepmerge": "^4.0.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "karma": "^4.3.0",
    "mocha": "^5.2.0",
    "node-sass-import": "^2.0.1",
    "npm-watch": "^0.6.0",
    "parcel": "^1.12.3",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
